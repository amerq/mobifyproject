    "babel-loader": "6.4.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.28.0",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-merlins-connector": "file:app/utils/eslint-plugin-merlins-connector",
    "eslint-plugin-react": "6.10.3",
    "exports-loader": "0.6.4",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "html-to-json": "0.6.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy": "1.15.1",
    "http-server": "0.9.0",
    "ignore-styles": "5.0.1",
    "imagemin-pngquant": "5.0.0",
    "imagemin-power-cli": "0.4.0",
    "ip": "1.1.5",
    "jest-cli": "19.0.2",
    "jsdoc": "^3.4.3",
    "jsdom": "9.12.0",
    "json-loader": "0.5.4",
    "lighthouse": "1.6.5",
    "minimist": "1.2.0",
    "mobify-code-style": "2.8.1",
    "nightwatch": "0.9.8",
    "nightwatch-commands": "2.1.0",
    "node-sass": "4.5.2",
    "postcss-loader": "1.3.3",
    "progressive-web-sdk": "~0.17.0",
    "prompt": "1.0.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.2",
    "react-styleguidist": "4.1.0",
    "redux-form": "6.1.1",
    "rimraf": "2.6.1",
    "sandy-tracking-pixel-client": "^0.1.21",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "susy": "2.2.12",
    "svg-sprite": "1.3.6",
    "svgo": "0.7.2",
    "text-loader": "0.0.1",
    "validator": "7.0.0",
    "walk": "2.3.9",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.0",
    "webpack-notifier": "^1.5.0"
  },
  "config": {
    "sdk_generator_version": 2,
    "min_lighthouse_score": 80,
    "file_size_limit": 2000000
  },
  "scripts": {
    "add:component": "sdk-generate component",
    "add:form": "sdk-generate form",
    "add:page": "sdk-generate page",
    "analyze-build": "MOBIFY_ANALYZE=true npm run prod:build",
    "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --dest=./app/static/svg/sprite-dist ./app/static/svg/sprite-source/*.svg",
    "clean:svgs": "svgo --enable='removeUselessStrokeAndFill' -f ./app/static/svg/sprite-source",
    "clean:images": "imagemin-power -v --recursive --plugin=pngquant \"./app/static/img/**/*\" --out-dir=.",
    "messaging:certificate": "sdk-messaging certificate",
    "messaging:testmessage": "sdk-messaging testmessage",
    "messaging:upload": "sdk-messaging upload",
    "copyright:lint": "copyright 'app/!(vendor|static)/!(docs)/**/*.js?(x)' 'worker/**/*.js?(x)' 'tests/**/*.js?(x)' 'webpack/**/*.js?(x)' 'service-worker-loader.js'",
    "copyright:fix": "copyright --fix --update 'app/!(vendor|static)/!(docs)/**/*.js?(x)' 'worker/**/*.js?(x)' 'tests/**/*.js?(x)' 'webpack/**/*.js?(x)' 'service-worker-loader.js'",
    "dev": "sdk-get-routes && sdk-create-hash-manifest && node ./dev-server/index.js",
    "im:docs": "jsdoc -c .jsdocrc",
    "lint": "npm run lint:js && npm run lint:sass && npm run copyright:lint",
    "lint:fix": "npm run lint:js -- --fix && npm run copyright:fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:sass": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "styleguide": "styleguidist --config webpack/styleguide.config.js server",
    "save-credentials": "sdk-save-credentials",
    "smoke-test": "bash ./tests/system/test-scripts/smoke-test.sh",
    "test": "cross-env NODE_ENV=test jest",
    "test:all": "npm run lint && npm test -- --coverage",
    "test:build-size": "node tests/system/test-scripts/verify-build-size.js",
    "test:check-lighthouse-score": "node tests/system/test-scripts/check-lighthouse-score.js",
    "test:e2e": "cross-env NODE_ENV=test node ./node_modules/nightwatch/bin/runner.js -c ./tests/system/nightwatch-config.js --suiteRetries 1",
    "test:pwa-prod": "bash ./tests/system/test-scripts/lighthouse/lighthouse-prod.sh",
    "test:pwa-preview": "bash ./tests/system/test-scripts/lighthouse/lighthouse-preview.sh",
    "test:server": "bash ./scripts/test-server.sh",
    "test:watch": "npm test -- --watch",
    "prod:build": "bash ./scripts/build.sh",
    "update-loader-routes": "sdk-get-routes"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!app/{loader.js,main.jsx}",
      "!app/static/js/*.js",
      "!app/vendor/*.js",
      "!app/containers/{templates,reducers}.js",
      "!app/**/{container,constants,selectors}.{js,jsx}",
      "!{dev-server,proxy,lighthouse,coverage,build,tests,webpack,worker}/**/*.js",
      "!styleguide/**/*.{js,jsx}",
      "!app/loader-routes.js",
      "!service-worker-loader.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 18,
        "functions": 35,
        "lines": 50,
        "statements": 47
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/tests/emptyStringMock.js"
    },
    "setupFiles": [
      "<rootDir>/tests/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/webpack/",
      "/tmp/",
      "/node_modules/",
      "/app/static/",
      "/app/vendor/",
      "/dev-server/",
      "/lighthouse/",
      "/coverage/"
    ],
    "testRegex": "\\.?(test|spec)\\.jsx?$"
  }
}
